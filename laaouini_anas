

def check_duplicate(l):
    visited = set()
    has_duplicate = False
    temp=[]
    for i in range(len(l)):
        if l[i] in visited:
            pass
        elif l.count(l[i]) == 1:
            visited.add(l[i])
        elif l.count(l[i]) > 1:
            temp = temp+[i]



    return temp #returning the index of the the duplicated element








def hackaton(sd,p):
    n=len(sd)
    temp1=check_duplicate(sd)
    temp2=[]
    maxv=0#on va avoir le max de profit du du duplicated elements
    i=temp1[0]
    for i in range(len(temp1)):
        temp2=[p[temp1[i]]]+[]


    maxv=max(temp2)
    return temp2
